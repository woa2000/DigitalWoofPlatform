# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Development Environment
NODE_ENV=development

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase Backend Configuration (for server-side operations)
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Supabase JWT Secret (para verificação no backend)
SUPABASE_JWT_SECRET=your_supabase_jwt_secret_here

# Database Configuration (REQUIRED for Drizzle ORM)
# Get this from your Supabase project settings > Database > Connection string > URI
# Format: postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres
DATABASE_URL=postgresql://postgres:your-password@db.your-project-ref.supabase.co:5432/postgres

# Server Configuration
PORT=5000

# Session Configuration (se necessário)
# SESSION_SECRET=your_session_secret_here